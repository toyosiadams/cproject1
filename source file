#include <iostream>
#include <iomanip>
#include "Car.h"
#include <string>

using namespace std;
int main()
{

	Car Car("honda", 2022);
	for (int i = 0; i < 5; i++)
	{
		cout << "Current speed: " << Car.accelerate() << endl;
		cout << "Accelerating..." << endl;
	}

	for (int i = 1; i < 6; i++)
	{
		cout << "Current speed: " << Car.brake() << endl;
		cout << "Braking..." << endl;
	}
}

	
